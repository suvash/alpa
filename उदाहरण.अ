; अङ्क-औ
(फलन '(श्रेणी अ)
      '(छनौट
	'((== अ ०) "शुन्य")
	'((== अ १) "प्रथम")
	'((== अ २) "दोस्रो")
	'((== अ ३) "तेस्रो")
	'((== अ ४) "चौथो")
	'((== अ ५) "पाँचौ")
	'((== अ ६) "छैठौँ")
	'((== अ ७) "सातौँ")
	'((== अ ८) "आठौँ")
	'((== अ ९) "नवौँ")
	'((== अ १०) "दशौँ")
	'(नत्र       "अहिले छैनौँ")))


; बार
(फलन '(बार ब)
     '(अवस्था ब
	     '(१ "आइतबार")
	     '(२ "सोमबार")
	     '(३ "मंगलबार")
	     '(४ "बुधबार")
	     '(५ "बिहीबार")
	     '(६ "शुक्रबार")
	     '(७ "शनिबार")))

; फिबोनाच्ची
(फलन '(फिब अ)
     '(छनौट
       '((== अ ०) ०)
       '((== अ १) १)
       '(नत्र (+ (फिब (- अ १)) (फिब (- अ २))))))


(छाप (फिब ५))