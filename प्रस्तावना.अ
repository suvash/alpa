; कमेन्ट लेख्नलाई सुरुमा सेमिकलन प्रयोग गर्ने

; नील भनेको खाली
(नामक '(नील) '())

; फलन - फन्क्सन
(नामक '(फलन)
      (\ '(बुँदाहरु शरीर)
       '(नामक (शिर बुँदाहरु)
	      (\ (पुच्छर बुँदाहरु) शरीर))))

; बुँदाहरु पोको पार्ने - प्याक
(फलन '(पोको फ ऽ बुँदाहरु) '(फ बुँदाहरु))

; बुँदाहरुको पोको फुकाल्ने - अन्प्याक
(फलन '(फुकाल फ बुँदाहरु)
     '(बिस्तार (एकत्र (सुची फ) बुँदाहरु)))

; करी अन्करी
(नामक '(करी) फुकाल)
(नामक '(अन्करी) पोको)

; गर - लहरै कुराहरु गर्ने - डु
(फलन '(गर ऽ कुराहरु)
     '(यदि (== कुराहरु नील)
	   '(नील)
	   '(अन्तिम कुराहरु)))

; मानौ नयाँ वातावरण - लेट
(फलन '(मानौ कुरा)
     '(((\ '(_) कुरा) ())))

; सत्य गलत उल्टाउने - नट
(फलन '(हैन क)
     '(यदि (== क सत्य)
	   '(गलत)
	   '(यदि (== क गलत)
		 '(सत्य)
		 '(गलत))))

; अथवा - अर
(फलन '(अथवा क ख)
     '(यदि (== क सत्य)
	   '(सत्य)
	   '(यदि (== ख सत्य)
		 '(सत्य)
		 '(गलत))))

; साथै - यान्ड
(फलन '(साथै क ख)
     '(यदि (== क सत्य)
	   '(यदि (== ख सत्य)
		 '(सत्य)
		 '(गलत))
	   '(गलत)))

; बुदा पल्टाउने - फ्लिप
(फलन '(पल्टाउ फ क ख) '(फ ख क))

; रचना - कम्पोज
(फलन '(रचना फ ब क) '(फ (ब क)))

; प्रथम, द्वितिय, तृतिय
(फलन  '(प्रथम कहरु) '(बिस्तार (शिर कहरु)))
(फलन  '(द्वितिय कहरु) '(बिस्तार (शिर (पुच्छर कहरु))))
(फलन  '(तृतिय कहरु) '(बिस्तार (शिर (पुच्छर (पुच्छर कहरु)))))

; लम्बाई - लेन्त
(फलन '(लम्बाई कहरु)
     '(यदि (== कहरु नील)
	   '(०)
	   '(+ १ (लम्बाई (पुच्छर कहरु)))))

; औँ - एन्त
(फलन '(औँ अ कहरु)
     '(यदि (== अ ०)
	   '(प्रथम कहरु)
	   '(औँ (- अ १) (पुच्छर कहरु))))

; अन्तिम - लास्ट
(फलन '(अन्तिम कहरु)
     '(औँ (- (लम्बाई कहरु) १) कहरु))

; लेउ - टेक
(फलन '(लेउ अ कहरु)
  '(यदि (== अ ०)
    '(नील)
    '(एकत्र (शिर कहरु) (लेउ (- अ १) (पुच्छर कहरु)))))

; छोड - ड्रप
(फलन '(छोड अ कहरु)
     '(यदि (== अ ०)
	   '(कहरु)
	   '(छोड (- अ १) (पुच्छर कहरु))))

; फोर - स्प्लिट
(फलन '(फोर अ कहरु)
     '(सुची (लेउ अ कहरु) (छोड अ कहरु)))

; मा (छ कि छैन) - एलेम
(फलन '(मा अ कहरु)
     '(यदि (== कहरु नील)
	   '(गलत)
	   '(यदि (== अ (प्रथम कहरु))
		 '(सत्य)
		 '(मा अ (पुच्छर कहरु)))))

; लहरै - म्याप
(फलन '(लहरै फ कहरु)
     '(यदि (== कहरु नील)
	   '(नील)
	   '(एकत्र
	     (सुची (फ (प्रथम कहरु)))
	     (लहरै फ (पुच्छर कहरु)))))

; छान - फिल्टर
(फलन '(छान फ कहरु)
     '(यदि (== कहरु नील)
	   '(नील)
	   '(एकत्र
	     (यदि (फ (प्रथम कहरु))
		     '(शिर कहरु)
		     '(नील))
	     (छान फ (पुच्छर कहरु)))))

; पट्याउ - फोल्ड
(फलन '(पट्याउ फ अ कहरु)
     '(यदि (== कहरु नील)
	   '(अ)
	   '(पट्याउ फ (फ अ (प्रथम कहरु)) (पुच्छर कहरु))))

; जोड
(फलन '(जोड कहरु) '(पट्याउ + ० कहरु))
(फलन '(गुना कहरु) '(पट्याउ * १ कहरु))

; छनौट - सेलेक्ट
(फलन '(छनौट ऽ छहरु)
     '(यदि (== छहरु नील)
	   '(समस्या "कुनै छनौटसुची प्रदान गरिएन")
	   '(यदि (प्रथम (प्रथम छहरु))
		 '(द्वितिय (प्रथम छहरु))
		 '(फुकाल छनौट (पुच्छर छहरु)))))

; नत्र - अदरवाइज
(नामक '(नत्र) सत्य)

; अङ्क-औ
(फलन '(श्रेणी अ)
      '(छनौट
	'((== अ ०) "शुन्य")
	'((== अ १) "प्रथम")
	'((== अ २) "दोस्रो")
	'((== अ ३) "तेस्रो")
	'((== अ ४) "चौथो")
	'((== अ ५) "पाँचौ")
	'((== अ ६) "छैठौँ")
	'((== अ ७) "सातौँ")
	'((== अ ८) "आठौँ")
	'((== अ ९) "नवौँ")
	'((== अ १०) "दशौँ")
	'(नत्र       "अहिले छैनौँ")))

; अवस्था - केस
(फलन '(अवस्था अ ऽ कहरु)
     '(यदि (== कहरु नील)
	   '(समस्या "कुनै अवस्था प्रदान गरिएन")
	   '(यदि (== अ (प्रथम (प्रथम कहरु)))
		 '(द्वितिय (प्रथम कहरु))
		 '(फुकाल अवस्था (एकत्र (सुची अ) (पुच्छर कहरु))))))

; बार
(फलन '(बार ब)
     '(अवस्था ब
	     '(१ "आइतबार")
	     '(२ "सोमबार")
	     '(३ "मंगलबार")
	     '(४ "बुधबार")
	     '(५ "बिहीबार")
	     '(६ "शुक्रबार")
	     '(७ "शनिबार")))
