; कमेन्ट लेख्नलाई सुरुमा सेमिकलन प्रयोग गर्ने

; नील भनेको खाली उद्धरण
(नामक '(नील) '())

; फलन - फन्क्सन
(नामक '(फलन) (\ '(बुँदाहरु शरीर) '(
  नामक (पहिलो बुँदाहरु) (\ (बाँकी बुँदाहरु) शरीर))))

; बुँदाहरु पोको पार्ने - प्याक - अन्करी
(फलन '(पोको फ ऽ बुँदाहरु) '(फ बुँदाहरु))

; बुँदाहरुको पोको फुकाल्ने - अन्प्याक - करी
(फलन '(फुकाल फ बुँदाहरु)
  '(बिस्तार (एकत्र (उद्धरण फ) बुँदाहरु)))

; गर - लहरै कुराहरु गर्ने - डु
(फलन '(गर ऽ कुराहरु)
  '(यदि (== कुराहरु नील)
    '(नील)
    '(अन्तिम कुराहरु)))

; मानौ नयाँ वातावरण - लेट
(फलन '(मानौ कुरा)
  '(((\ '(_) कुरा) ())))

; सत्य गलत उल्टाउने - नट
(फलन '(हैन क)
  '(यदि (== क सत्य)
    '(गलत)
    '(यदि (== क गलत)
      '(सत्य)
      '(गलत))))

; अथवा - अर
(फलन '(अथवा क ख)
  '(यदि (== क सत्य)
    '(सत्य)
    '(यदि (== ख सत्य)
      '(सत्य)
      '(गलत))))

; साथै - यान्ड
(फलन '(साथै क ख)
  '(यदि (== क सत्य)
    '(यदि (== ख सत्य)
      '(सत्य)
      '(गलत))
    '(गलत)))

; बुदा पल्टाउने - फ्लिप
(फलन '(पल्टाउ फ क ख) '(फ ख क))

; रचना - कम्पोज
(फलन '(रचना फ ब क) '(फ (ब क)))

; प्रथम, द्वितिय, तृतिय
(फलन  '(प्रथम कहरु) '(बिस्तार (पहिलो कहरु)))
(फलन  '(द्वितिय कहरु) '(बिस्तार (पहिलो (बाँकी कहरु))))
(फलन  '(तृतिय कहरु) '(बिस्तार (पहिलो (बाँकी (बाँकी कहरु)))))

; लम्बाई - लेन्त
(फलन '(लम्बाई कहरु)
  '(यदि (== कहरु नील)
    '(०)
    '(+ १ (लम्बाई (बाँकी कहरु)))))

; औँ - एन्त
(फलन '(औँ अ कहरु)
  '(यदि (== अ ०)
    '(प्रथम कहरु)
    '(औँ (- अ १) (बाँकी कहरु))))

; अन्तिम
(फलन '(अन्तिम कहरु)
  '(औँ (- (लम्बाई कहरु) १) कहरु))