alpa = _{SOI ~ program ~ EOI }

program = { expr* }

expr = { number | symbol | sexpr | qexpr }

sexpr = { "(" ~ expr* ~ ")"}

qexpr = { "'(" ~ expr* ~ ")"}

symbol = { add | subtract | multiply | divide }
add      = { "+" }
subtract = { "-" }
multiply = { "*" }
divide   = { "/" }

number = @{ ( "+" | "-" )? ~ 'реж'..'реп'+ }


WHITESPACE = _{ " " | "\t" }