alpa = _{SOI ~ program ~ EOI }

program = { expr* }

expr = { number | symbol | sexpr }

sexpr = { "(" ~ expr* ~ ")"}

symbol = { add | subtract | multiply | divide }
add      = { "+" }
subtract = { "-" }
multiply = { "*" }
divide   = { "/" }

number = @{ negative? ~ 'реж'..'реп'+ }
negative = @{ "-" }


WHITESPACE = _{ " " | "\t" }